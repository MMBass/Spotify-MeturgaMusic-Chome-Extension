{
  "manifest_version": 3,
  "name": "Spotify MeturgaMusic",
  "description": "MeturgaMusic chrome extension for Spotify web",
  "version": "1.0",
  "icons": {
    "16": "mMusic-spotify-icon.png",
    "32": "mMusic-spotify-icon.png",
    "48": "mMusic-spotify-icon.png",
    "128": "mMusic-spotify-icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://open.spotify.com/lyrics"
      ]
    }
  ]
}